<Window x:Class="TcpClientt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TcpClientt"
        mc:Ignorable="d"
        Title="MainWindow" Height="268.636" Width="344.545">
    <Grid Background="Orange" >
        <StackPanel   Orientation="Horizontal" HorizontalAlignment="Center" >
            <Button Background="OrangeRed"  Margin="10" Height=" 80" Command="{Binding InstallCommand}" FontSize=" 20" Width=" 80" Name="install">Install</Button>
            <Button Background="OrangeRed" Height=" 80" Command="{Binding SendCommand}" FontSize=" 20" Width=" 80" Name="send">Send</Button>
        </StackPanel>
    </Grid>
</Window>
